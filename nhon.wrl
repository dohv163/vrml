DEF view Viewpoint {
    position 0 0 -300
    orientation 0 1 0 3.14
}
DEF time TimeSensor {
    loop TRUE
    enabled FALSE
    cycleInterval  2
}
DEF tinhtien PositionInterpolator {
    key [0,0.5,1]
    keyValue [0 0 100,0 0 30]
}
ROUTE time.fraction_changed TO tinhtien.set_fraction
ROUTE tinhtien.value_changed TO view.position


